{
    "extends": [
      "@rocketseat/eslint-config/node"
    ],
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "endOfLine": "auto",
          "tabWidth": 2,
          "useTabs": true,
          "singleQuote": false,
          "semi": false
        }
      ],
      "indent": ["error", "tab"],
      "quotes": ["error", "double"],
      "semi": ["error", "never"]
    }
  }